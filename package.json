    },
    "author": "Hans",
    "license": "ISC",
    "dependencies": {
        "@umijs/types": "^3.2.22",
        "antd": "^4.6.6",
        "classnames": "^2.2.6",
        "dumi": "^1.0.34",
        "hooooks": "^1.0.21",
        "prism-react-renderer": "^1.1.1"
